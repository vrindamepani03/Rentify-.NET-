<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApplication2</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css"/>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js"></script>
    <link rel="stylesheet" href="~/WebApplication2.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm ">
            <div class="container-fluid">
                <div class="div-1111">
                    <a asp-controller="signupController1" asp-action="home" class="img-link">
                            <img loading="lazy"
                                 srcset="https://cdn.builder.io/api/v1/image/assets/TEMP/eb887c5a0fbdaab761bf3d8badf4443f8f8641bbfacac742b16d5e5237a72da0?apiKey=785d7864d136470c97c9054c58601580&width=100 100w, https://cdn.builder.io/api/v1/image/assets/TEMP/eb887c5a0fbdaab761bf3d8badf4443f8f8641bbfacac742b16d5e5237a72da0?apiKey=785d7864d136470c97c9054c58601580&width=200 200w, https://cdn.builder.io/api/v1/image/assets/TEMP/eb887c5a0fbdaab761bf3d8badf4443f8f8641bbfacac742b16d5e5237a72da0?apiKey=785d7864d136470c97c9054c58601580&width=400 400w, https://cdn.builder.io/api/v1/image/assets/TEMP/eb887c5a0fbdaab761bf3d8badf4443f8f8641bbfacac742b16d5e5237a72da0?apiKey=785d7864d136470c97c9054c58601580&width=800 800w, https://cdn.builder.io/api/v1/image/assets/TEMP/eb887c5a0fbdaab761bf3d8badf4443f8f8641bbfacac742b16d5e5237a72da0?apiKey=785d7864d136470c97c9054c58601580&width=1200 1200w, https://cdn.builder.io/api/v1/image/assets/TEMP/eb887c5a0fbdaab761bf3d8badf4443f8f8641bbfacac742b16d5e5237a72da0?apiKey=785d7864d136470c97c9054c58601580&width=1600 1600w, https://cdn.builder.io/api/v1/image/assets/TEMP/eb887c5a0fbdaab761bf3d8badf4443f8f8641bbfacac742b16d5e5237a72da0?apiKey=785d7864d136470c97c9054c58601580&width=2000 2000w, https://cdn.builder.io/api/v1/image/assets/TEMP/eb887c5a0fbdaab761bf3d8badf4443f8f8641bbfacac742b16d5e5237a72da0?apiKey=785d7864d136470c97c9054c58601580&"
                                 class="img" />
                        </a>
                        <div class="div-2222">
                        <div class="div-3333"><a asp-controller="signupController1" asp-action="ListProperty" class="btn btn-primary">List your property</a></div>
                            <div class="div-4444"><a asp-controller="signupController1" asp-action="SignUp" class="btn btn-primary">Sign Up</a></div>
                            <div class="div-5555"><a asp-controller="signupController1" asp-action="Login" class="btn btn-primary">Login</a></div>
                        </div>                       
                </div>
            </div>
        </nav>
    </header>
    <style>
 body {
        background-color: #bdcce0;
        overflow: auto;
    }
        
.div-1111{
    background-color: #dfe8f3;
    display: flex;
    width: 100%;
    justify-content: space-between;
    gap: 20px;
    font-size: 20px;
    color: #000;
    font-weight: 400;
    padding: 8px 64px 8px;
}
@@media (max-width: 991px) {
    .div-1111 {
        max-width: 100%;
        flex-wrap: wrap;
        padding: 0 20px;
    }
}
.img {
    aspect-ratio: 1.02;
    object-fit: contain;
    object-position: center;
    width: 81px;
    border-radius: 50%;
}

.div-2222 {
    display: flex;
    align-items: center;
    justify-content: space-between;
    gap: 20px;
    margin: auto 0;
}
@@media (max-width: 991px) {
    .div-2222 {
      max-width: 100%;
      flex-wrap: wrap;
    }
  }
.div-3333 {
    font-family: Inria Sans, sans-serif;
    border-radius: 10px;
    background-color: #bdcce0;
    align-self: stretch;
    flex-grow: 1;
    justify-content: center;
    padding: 7px 10px;
}

.div-4444 {
    color: #101112;
    font-family: Inria Sans, sans-serif;
    align-self: stretch;
    margin: auto 0;
}

.div-5555 {
    font-family: Inria Sans, sans-serif;
    align-self: stretch;
    margin: auto 0;
}
.btn.btn-primary {
    background-color: transparent;
    color: #101112; 
    border: none;
}
.btn.btn-primary:hover {
        background-color: transparent;
        color: #101112; 
        border: none;
    }
    .btn.btn-primary:active {
        outline: none;
        border: none; 
        box-shadow: none; 
        border-color: #101112; 
    }

    </style>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - WebApplication2 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
